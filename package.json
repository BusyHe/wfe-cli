{
    "name": "wfe-cli",
    "version": "1.3.1",
    "description": "Front end scaffolding tool",
    "private": false,
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "preferGlobal": true,
    "scripts": {
        "lint": "eslint .",
        "postinstall": "husky install",
        "prepack": "pinst --disable",
        "postpack": "pinst --enable"
    },
    "bin": {
        "wfe": "./cli.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/wfe-cli/wfe-cli.git"
    },
    "keywords": [
        "wf"
    ],
    "author": "busyhe",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/wfe-cli/wfe-cli/issues"
    },
    "homepage": "https://github.com/wfe-cli/wfepinst-cli#readme",
    "dependencies": {
        "async": "^3.2.1",
        "axios": "^0.26.0",
        "chalk": "^4.1.2",
        "coffee-script": "1.12.7",
        "commander": "^8.2.0",
        "consolidate": "^0.16.0",
        "dayjs": "^1.10.7",
        "download-git-repo": "^3.0.2",
        "handlebars": "^4.7.7",
        "ini": "^2.0.0",
        "inquirer": "^8.1.5",
        "inquirer-checkbox-plus-prompt": "^1.0.1",
        "metalsmith": "^2.3.0",
        "minimatch": "^3.0.4",
        "multimatch": "^5.0.0",
        "ora": "^5.1.0",
        "read-metadata": "^1.0.0",
        "rimraf": "^3.0.2",
        "semver": "^7.3.5",
        "tildify": "^3.0.0",
        "uid": "2.0.0",
        "user-home": "^2.0.0",
        "validate-npm-package-name": "^3.0.0"
    },
    "devDependencies": {
        "@busyhe/eslint-config": "^1.0.0",
        "@semantic-release/changelog": "^6.0.1",
        "@semantic-release/git": "^10.0.1",
        "chai": "^4.3.4",
        "commitizen": "^4.2.4",
        "cz-conventional-changelog": "^3.3.0",
        "eslint": "^7.32.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-n": "^15.2.3",
        "eslint-plugin-promise": "^6.0.0",
        "execa": "^5.1.1",
        "husky": "^7.0.2",
        "lint-staged": "^13.0.2",
        "mocha": "^9.2.0",
        "pinst": "^3.0.0",
        "semantic-release": "^19.0.2"
    },
    "engines": {
        "node": ">=16.0.0"
    },
    "eslintConfig": {
        "extends": "@busyhe",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 2017
        }
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx,vue}": "eslint --fix"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    }
}
